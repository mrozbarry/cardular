{
  "name": "cardular",
  "version": "0.0.1",
  "description": "A multiplayer framework for playing cards",
  "main": "public/index.html",
  "scripts": {
    "test": "mocha --opts spec/mocha.conf spec/**/*.spec.js",
    "test:rules": "firebase-bolt config/rules.bolt --output rules.json; ./node_modules/.bin/targaryen --verbose rules.json targaryen/test.json; rm rules.json",
    "start": "webpack-dev-server --inline --progress --quiet --no-info --config config/webpack.hmr.js --host 0.0.0.0 --port 3000"
  },
  "author": "Alex Barry",
  "license": "MIT",
  "peerDependencies": {
    "firebase-tools": "*",
    "firebase-bolt": "*",
    "webpack-dev-server": "*"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-react-display-name": "*",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-node5": "^11.0.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "css-loader": "^0.23.1",
    "firebase": "^2.4.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.8.1",
    "jdenticon": "^1.3.2",
    "jsdom": "^7.2.2",
    "json-loader": "^0.5.4",

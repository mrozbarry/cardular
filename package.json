    "mocha": "^2.3.4",
    "pleasejs": "^0.4.2",
    "react": "^0.14.3",
    "react-addons-test-utils": "^0.14.6",
    "react-color": "^2.0.0",
    "react-dom": "^0.14.3",
    "react-mini-router": "^2.0.0",
    "react-transform-hmr": "^1.0.1",
    "standard": "^5.4.1",
    "standard-loader": "^2.0.0",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.5.1",
    "superagent": "^1.7.2",
    "uuid": "^2.0.1"
